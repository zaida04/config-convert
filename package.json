{
  "name": "config-convert1",
  "version": "0.0.2-dev",
  "description": "ðŸš€ Have a bunch of config files written in JS that you don't feel like converting to JSON by hand? Use this to batch convert them!",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "bin": "dist/bin.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint src/ --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest --config jest.config.json",
    "test:build": "npm run build && npx jest --config jest.config.json",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "husky install",
    "test:js": "npx ts-node src/bin.ts -f dummy/test-config.json -o dummy/output-1.js",
    "test:json": "npx ts-node src/bin.ts -f dummy/test-config.js -o dummy/output-1.json"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/generator": "^7.13.0",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@sapphire/eslint-config": "^3.2.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.7.6",
    "@types/prettier": "^2.3.2",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-replace-ts-export-assignment": "^0.0.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.4.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "prettier": "^2.3.2",
    "yargs": "^17.1.1"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaida04/config-convert.git"
  },
  "author": "Zaid \"Nico\"",
